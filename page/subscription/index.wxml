<view>
  <view class="weui-panel">
    <view class="weui-panel__bd">
      <navigator wx:for-items="{{list}}" wx:key="{{item.id}}" url="pages/owner/index?owner={{item.owner}}&repo={{item.repo}}" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
        <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
          <image class="weui-media-box__thumb" src="{{item.avatar_url}}" />
        </view>
        <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
          <view class="weui-media-box__title">{{item.name}}</view>
          <view class="weui-media-box__desc">{{item.login}}</view>
          <view class="weui-media-box__desc">{{item.email}}</view>
        </view>
      </navigator>
    </view>
  </view>
  <view class="weui-btn-area">
    <button class="weui-btn" type="primary" bindtap="add">添加订阅</button>
  </view>
</view>